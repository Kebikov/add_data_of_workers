{
	"manifest_version": 3,
	"name": "Adding workers",
	"description": "For add workers of ozon",
	"version": "1.0",
	"action": {
		"default_popup": "hello.html"
	},
	"icons": {
		"16": "images/16.png",
		"32": "images/32.png",
		"48": "images/48.png",
		"128": "images/128.png"
	},
    "permissions": [
        "activeTab", "scripting",  "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}
